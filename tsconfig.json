{
  "compilerOptions": {
    "target": "es5",
    "outDir": "dist",
    "lib": ["es6", "dom", "es2016", "es2017", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": ["src"]
}
